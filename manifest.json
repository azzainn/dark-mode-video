{
    "manifest_version": 3,
    "name": "Dark Mode Video",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {},
    "content_scripts": [
      {
        "matches": ["*://*.panopto.com/*"],
        "js": ["contentScript.js"],
        "run_at": "document_idle"
      }
    ]
}
  